    <validationRules>
        <fullName>Checkbox_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND ( isStatusForCompletion__c = false,   isStatusForEmail__c  = false, isStatusForProgress__c  = false)</errorConditionFormula>
        <errorMessage>Atleast one checkbox must be checked to create the record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Email_Template_Name_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND(isStatusForEmail__c = true , OR(ISBLANK( Email_Template_Name__c ), ISBLANK( Status__c )))</errorConditionFormula>
        <errorMessage>Email Template Name and status can not be left blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Check</fullName>
        <active>true</active>
        <errorConditionFormula>AND( OR(  isStatusForCompletion__c = true,  isStatusForProgress__c = true) ,ISBLANK( Status__c ))</errorConditionFormula>
        <errorDisplayField>Status__c</errorDisplayField>
        <errorMessage>Status cannot be left blank.</errorMessage>
    </validationRules>
