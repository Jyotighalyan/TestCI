        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/12.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/12.0/apex.js&quot;)}  

if (&apos;{!Contact.B2B_ULM_User_Id__c}&apos; == &apos;&apos;){
 alert (&apos;ULM User Id should not be blank&apos;)
}
else if(&apos;{!Contact.B2B_Self_Care_User_Status__c}&apos; != &apos;Active&apos;){
alert (&apos;Self Care User Status is not Active&apos;)
}
else{
var result = sforce.apex.execute(&quot;SendDataToULM&quot;,&quot;sendData&quot;,{ulmUserId:&quot;{!Contact.B2B_ULM_User_Id__c}&quot;}); 
if(result == 200){
alert(&quot;Self care portal data synced&quot;); 
}
else{
alert(&quot;Self care portal data not synced&quot;); 
}
}</url>
