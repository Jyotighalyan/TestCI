    <fields>
        <fullName>B2B_Incomplete_Contact__c</fullName>
        <externalId>false</externalId>
        <formula>IF( AND (
      OR(  
      OR(ISBLANK(Email), ISNULL(Email)),       
      OR(ISBLANK( Phone ), ISNULL(Phone))  
      ),
      ISPICKVAL( B2B_Bounced_Category_pklst__c , &apos;Hard Bounce&apos;)
        ), 
      IMAGE($Label.B2B_Bounced_Incomplete_Contact , &quot;!&quot;, 30,30) ,
	  ( IF( 
			OR(  
			OR(ISBLANK(Email), ISNULL(Email)), 
			OR(ISBLANK( Phone ), ISNULL(Phone))  
		   ), 
      IMAGE($Label.B2B_Exclamation_Mark_Bounce , &quot;!&quot;, 30,30) ,
	  
        (IF(ISPICKVAL( B2B_Bounced_Category_pklst__c , &apos;Hard Bounce&apos;), 
      IMAGE($Label.B2B_Bounced_Contact , &quot;!&quot;, 30,30) , NULL))
)
)
)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Please Note</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <type>Text</type>
        <unique>false</unique>
