    <fields>
        <fullName>UM_ProjectLoadTotal__c</fullName>
        <externalId>false</externalId>
        <formula>UM_Quantity__c *  UM_ContractTerm__c *  (UM_MRR__c +  UM_OTC__c)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Project Load (Total)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
    <fields>
        <fullName>UM_ProjectLoadWeighted__c</fullName>
        <description>Translates the change picklist value to string and than to integer and calculates the weighted project load.</description>
        <externalId>false</externalId>
        <formula>VALUE(
CASE( UM_Chance__c ,
 &apos;0%&apos;, TEXT(0),
 &apos;10%&apos;, TEXT(0.10),
 &apos;20%&apos;, TEXT(0.20),
 &apos;30%&apos;, TEXT(0.30),
 &apos;40%&apos;, TEXT(0.40),
 &apos;50%&apos;, TEXT(0.50),
 &apos;60%&apos;, TEXT(0.60),
 &apos;70%&apos;, TEXT(0.70),
 &apos;80%&apos;, TEXT(0.8),
 &apos;90%&apos;, TEXT(0.9),
 &apos;100%&apos;,TEXT(1),
TEXT(0))
)
* UM_ProjectLoadTotal__c</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Project Load (Weighted)</label>
        <precision>18</precision>
        <required>false</required>
        <scale>2</scale>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
