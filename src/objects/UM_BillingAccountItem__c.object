    <validationRules>
        <fullName>Cannot_Create_new_BAI_For_Inactive_BA</fullName>
        <active>true</active>
        <errorConditionFormula>OR(ISPICKVAL(UM_BillingAccount__r.csconta__Status__c, &apos;Deactivation Pending&apos;),ISPICKVAL(UM_BillingAccount__r.csconta__Status__c, &apos;Inactive&apos;),ISPICKVAL(UM_BillingAccount__r.csconta__Status__c, &apos;New&apos;),ISPICKVAL(UM_BillingAccount__r.csconta__Status__c, &apos;Pending activation&apos;),ISPICKVAL(UM_BillingAccount__r.csconta__Status__c, &apos;Pending disconnection&apos;),ISPICKVAL(UM_BillingAccount__r.csconta__Status__c, &apos;Disconnected&apos;))</errorConditionFormula>
        <errorMessage>You cannot create or edit Billing account items on Billing accounts with status other than &quot;Active&quot; or &quot;Deactivation Requested&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>NRC_Reason_Restriction_for_Kenan_Records</fullName>
        <active>false</active>
        <description>Restrict User to select  manually NRC Reason &quot;Adjustment from Kenan&quot; . This value is meant to be used form Adjustment records are created in kenan and fed to SF.</description>
        <errorConditionFormula>AND( UM_Received_from_Billing_System__c == false, ISPICKVAL( UM_NRC_Reason__c , &apos;Adjustment from Kenan&apos;) )</errorConditionFormula>
        <errorMessage>NRC Reason Value &quot;Adjustment from Kenan&quot;  is not valid unless records are fed from Kenan. Please Select any other Listed reason.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>UM_Manager_cannot_be_empty_for_approval</fullName>
        <active>true</active>
        <errorConditionFormula>RecordType.Name==&apos;Adjustment&apos; &amp;&amp;  ISBLANK(Owner:User.ManagerId)&amp;&amp;  UM_Received_from_Billing_System__c ==false</errorConditionFormula>
        <errorMessage>There is no Manager for the current submitter. Please set the Manager for the current user submitting the record.</errorMessage>
    </validationRules>
</CustomObject>
