    <fields>
        <fullName>UM_OpenCDR__c</fullName>
        <description>This gives link to open CDR with extra parameter in URL</description>
        <externalId>false</externalId>
        <formula>IF( UM_IsCDRAvailable__c, IF(CONTAINS(UM_InvoiceNumber__c,&apos;-&apos;), 
HYPERLINK(&quot;/apex/UM_Documentum?invoiceno=&quot; + LEFT(UM_InvoiceNumber__c,FIND(&apos;-&apos;,UM_InvoiceNumber__c)-1) + &apos;&amp;itemised_bill=1&apos;, &quot;Open CDR&quot;), 
HYPERLINK(&quot;/apex/UM_Documentum?invoiceno=&quot; + UM_InvoiceNumber__c + &apos;&amp;itemised_bill=1&apos;, &quot;Open CDR&quot;)),&apos;&apos;)</formula>
        <label>Open CDR</label>
        <required>false</required>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>UM_OpenPDF__c</fullName>
