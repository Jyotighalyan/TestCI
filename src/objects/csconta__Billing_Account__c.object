    <webLinks>
        <fullName>Deactivate_Billing_Account</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Deactivate Billing Account</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/29.0/apex.js&quot;)}
 
var roleName =&apos;{!$UserRole.Name}&apos;;
var profileName = &apos;{!$Profile.Name}&apos;;

if ((profileName == &apos;UM_CSR_Profile&apos;) &amp;&amp; (roleName !=&apos;B2B BS SME Care&apos; &amp;&amp; roleName !=&apos;B2B Billing Operations&apos;))
{
  alert(&apos;Please check your role. It should be either B2B BS SME CARE or B2B BS Billing&apos;);
}

else
{
var billingAcc = new sforce.SObject(&quot;csconta__Billing_Account__c&quot;);
 billingAcc.ID = &apos;{!csconta__Billing_Account__c.Id}&apos;;
 billingAcc.Status = &apos;{!csconta__Billing_Account__c.csconta__Status__c}&apos;;

var result = sforce.apex.execute(&quot;UM_Deactivate_Billing_Account&quot;,&quot;deactivateBA&quot;,{billingId:billingAcc.ID});


if (result == &apos;Deactivate&apos;){
window.location.reload();
}
else if (result == &apos;Error&apos;){
alert(&apos;Please check the conditions: For all the linked services deactivation date should not be null and Status should not be equal to active&apos;);
}
else if (result == &apos;Already Deactivated&apos;){
alert(&apos;Status is already - Deactivation Requested&apos;);
}
else if (result == &apos;No linked Services&apos;){
alert(&apos;No Subscriptions/Services present&apos;);
}
else if (result == &apos;Already Deact Pending&apos;){
alert (&apos;Status is already Deactivation Pending&apos;);
}
else if (result == &apos;Already Inactive&apos;){
alert (&apos;Status is already Inactive&apos;);
}

if (result == &apos;Invoke API&apos;){
if( billingAcc.Status == &apos;Deactivation Requested&apos;)
{
 alert(&apos;Status is already - Deactivation Requested&apos;);
}
sforce.apex.execute(&quot;UM_KenanAccountSyncCtrl&quot;,&quot;invokeACAPI&quot;,{billingId:billingAcc.ID});
window.location.reload();
}
}</url>
