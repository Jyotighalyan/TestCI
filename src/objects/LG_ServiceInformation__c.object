<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <customSettingsType>List</customSettingsType>
    <description>This custom setting type is used to store mappings from REST API service names to user-friendly service names. The standard name field should contain the REST API name of the service.</description>
    <enableFeeds>false</enableFeeds>
    <fields>
        <fullName>LG_ServiceName__c</fullName>
        <caseSensitive>false</caseSensitive>
        <description>Name of service.</description>
        <externalId>false</externalId>
        <label>Service Name</label>
        <length>255</length>
        <required>true</required>
        <trackTrending>false</trackTrending>
        <type>Text</type>
        <unique>true</unique>
    </fields>
    <label>Service Information</label>
    <visibility>Protected</visibility>
</CustomObject>
